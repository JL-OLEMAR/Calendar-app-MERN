{
  "name": "calendar-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/JL-OLEMAR/Calendar-app-MERN/"
  },
  "bugs": "https://github.com/JL-OLEMAR/Calendar-app-MERN/issues",
  "homepage": "https://Calendar-Ole.up.railway.app/",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "standard --fix src/"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.0",
    "axios": "1.1.3",
    "date-fns": "2.29.3",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-big-calendar": "1.5.0",
    "react-datepicker": "4.8.0",
    "react-dom": "18.2.0",
    "react-modal": "3.16.1",
    "react-redux": "8.0.5",
    "react-router-dom": "6.4.3",
    "react-toastify": "9.1.1"
  },
  "devDependencies": {
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@vitejs/plugin-react": "2.2.0",
    "eslint": "8.26.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "standard": "17.0.0",
    "vite": "3.2.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:react-hooks/recommended",
      "./node_modules/standard/eslintrc.json"
    ],
    "rules": {
      "space-before-function-paren": [
        "warn",
        {
          "anonymous": "always",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "multiline-ternary": [
        "warn",
        "never"
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
